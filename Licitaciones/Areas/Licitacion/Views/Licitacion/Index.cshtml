        function vaciarImprimir() {
            $("#nombreObraI").prop("checked", false);
            $('#expedienteI').prop("checked", false);
            $('#publicacionI').prop("checked", false);
            $('#organismoI').prop("checked", false);
            $('#montoI').prop("checked", false);
            $('#etapaI').prop("checked", false);
