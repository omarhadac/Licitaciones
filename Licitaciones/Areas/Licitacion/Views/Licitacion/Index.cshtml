        });
        function buscarTieneActa(idObra) {
            var urltmp = "/api/Acta/buscarTieneActa?idObra=" + idObra;
            fetch(urltmp, { method: "GET" })
                .catch(() => {
                    alert('Ocurrio un error');
                })
                .then(res => res.json())
                .then(data => {
                    if (data == 1) {
                        window.location.href = '/api/Acta/DescargarArchivo?idObra=' + idObra + '&idCategoria=1';
                    }
                    else {
                        $('#idObraActa').val(idObra);
                        mostrarPopupActa(idObra);
                    }
                })
        }

